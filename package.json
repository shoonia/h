{
  "name": "jsx-dom-runtime",
  "version": "0.3.0",
  "description": "JSX syntax for DOM Element",
  "main": "jsx-runtime/jsxRuntime.cjs.js",
  "module": "jsx-runtime/jsxRuntime.module.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/** tests/**",
    "test": "jest",
    "build": "rollup -c",
    "pretest": "npm run build",
    "posttest": "npm run lint && size-limit",
    "size": "npm run build && size-limit"
  },
  "dependencies": {
    "@babel/core": "^7.13.8",
    "@babel/helper-plugin-utils": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.12.17"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "fs-extra": "^9.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "rollup": "^2.40.0",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^4.10.1",
    "typescript": "^4.2.3"
  },
  "files": [
    "src",
    "jsx-runtime",
    "babel-preset",
    "README",
    "LICENSE"
  ],
  "keywords": [
    "jsx",
    "jsx-dom",
    "jsx-runtime"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "size-limit": [
    {
      "path": "jsx-runtime/jsxRuntime.esm.js",
      "limit": "495 B"
    },
    {
      "path": "jsx-runtime/jsxRuntime.cjs.js",
      "limit": "491 B"
    },
    {
      "path": "jsx-runtime/jsxRuntime.module.js",
      "limit": "502 B"
    }
  ],
  "engines": {
    "node": "14",
    "npm": "7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shoonia/jsx-dom-runtime.git"
  },
  "bugs": {
    "url": "https://github.com/shoonia/jsx-dom-runtime/issues"
  },
  "author": {
    "name": "Alexander Zaytsev",
    "email": "zaytsev126@gmail.com",
    "url": "https://twitter.com/_shoonia"
  },
  "homepage": "https://github.com/shoonia/jsx-dom-runtime",
  "license": "MIT"
}
